<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Ex02 jQuery</title>
  <style>
    table { border-collapse: collapse; margin: 20px auto; }
    td { border: 1px solid black; padding: 8px; text-align: center; }
  </style>
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
  <script src="script.js"></script>
</head>
<body>
  <h1 style="text-align:center;">Multiplication Table</h1>
  <table id="table"></table>
</body>
</html>
$(document).ready(function () {
  const $table = $("#table");

  for (let i = 1; i <= 9; i++) {
    const $row = $("<tr></tr>");
    for (let j = 1; j <= 9; j++) {
      $row.append("<td>" + (i * j) + "</td>");
    }
    $table.append($row);
  }
});
